
/*
	Cores Paleta
	Verde escuro = #022601
	Verde claro = #52A202
	Amarelo escuro = #D9CB04
	Amarelo claro = #FDFE15
	Marron = #BF3604
*/

/*Reset CSS*/
* {
	margin: 0;
	padding: 0;
	font-family: arial, helvetica, trebuchet;
}

ul {
	list-style: none;
}

h3 {
	color: #868282;
	font-size: 16px;
	border-bottom: 1px dotted #868282;
}
.clr{
	clear: both;
}
/**/
body{
	height: 100%;
}
select{
	padding: 5px;
}
.wrapper {
	width: 100%;
	position: relative;
	min-height: 100%;
}

.header {
	background-color: #91C95A;
	border-bottom: 1px dotted #ccc;
}

.topo {
	width: 98%;
	margin: 0 auto;
}

.logo {
	width: 20%;
	display: inline-block;
	vertical-align: middle;
	text-align: center;
}

.img-logo {
	max-width: 75px;
	height: auto;
	padding: 5px 20px;
}

.menu {
	width: 60%;
	display: inline-block;
	vertical-align: middle;
	margin-left: 20px;
}

.ul-menu {
	display: block;
	vertical-align: middle;
}

.ul-menu li {
	display: inline-block;
}

.ul-menu li a {
	padding: 10px 30px;
	color: #022601;
	border: 1px dotted #ffffff;
	font-size: 16px;
	font-weight: bold;
	text-decoration: none;
}

.ul-menu li a:hover {
	background-color: #022601;
	color: #ffffff;
}

.busca {
	display: inline-block;
	vertical-align: middle;
}

.busca ul {
	list-style: none;
}

.busca ul li {
	background-color: #fff;
	padding: 7px;
	border-radius: 10px;
}

.busca ul li input {
	background: none;
	border: none;
	height: 25px;
}

.banner-topo {
	width: 100%;
	height: auto;
}

.banner-topo img {
	max-width: 100%;
	height: auto;
}

/*GERAL*/
.geral {
	width: 100%;
	margin: 0 auto;
	background-color: #ffffff;
	text-align: center;
	padding-bottom: 30px;
}

.geral div {
	vertical-align: top;
	padding: 5px;
}

/*Estilo coluna direita*/
.left {
	width: 20%;
	display: inline-block;
	padding: 10px;
}

.login {
	padding: 10px;
	margin-bottom: 20px;
	border: 1px solid #cccccc;
}

.login h3 {
	text-align: left;
}

.login label {
	display: block;
}

.login input {
	padding: 5px;
	margin-left: 5px;
}

.login .bt-logar input { 
	width: 65%;
	font-weight: bold;
	cursor: pointer;
	color: #ffffff;
	border: none;
	border-radius: 5px;
	background-color: #52A202;
}

.login .bt-logar p{
	margin-top: 10px;
	font-size: 12px;
	font-style: italic;
}

.login .bt-logar input:hover {
	background-color: #78AA39;
	background: linear-gradient(#78AA39, #78AA39); /* Standard syntax */
}

.link-cad{
	margin-bottom: 10px;
	border: 1px solid #cccccc;
	padding: 20px !important;
}

.link-cad p{
	font-size: 14px;
}

.link-cad a{
	width: 80%;
	font-weight: bold;
	cursor: pointer;
	color: #ffffff;
	border: none;
	padding: 10px;
	font-size: 14px;
	text-decoration: none;
	border-radius: 5px;
	background-color: #52A202;
}
.link-cad .aut-facebook a{
	background-color: #ffffff;
}

.aut-facebook a img{
	max-width: 150px;
}

.duvidas-frequentes {
	padding: 10px;
	margin-bottom: 20px;
	border: 1px solid #cccccc;
}

.duvidas-frequentes h3 {
	text-align: left;
}

.duvidas-lista li a {
	display: block;
	padding: 5px;
	font-size: 12px;
	color: #022601;
}

.duvidas-lista li {
	text-align: justify;
}

.center {
	width: 52%;
	display: inline-block;
	padding: 10px 0;
}

/*Bloco de Perguntas*/
.bloco-perguntas {
	width: 100%;
}

.bloco-perguntas h3 {
	text-align: left;
}

.pergunta {
	padding: 10px 0;
	background-color: #FFFFCB;
	margin: 5px 0;
}

.pergunta div {
	display: inline-block;
}

.pergunta .id-pergunta {
	width: 5%;
}

.pergunta .titulo-pergunta {
	width: 45%;
}

.pergunta .titulo-pergunta a {
	color: #022601;
	font-style: italic;
	font-weight: bold;
}

.pergunta .data-pergunta {
	width: 20%;
}

.pergunta .link-resposta {
	width: 25%;
}

.pergunta .link-resposta a {
	text-decoration: none;
	padding: 5px;
	border-radius: 3px;
	color: #fff;
	background-color: #52A202;

	/*	Amarelo escuro = #D9CB04
	Amarelo claro = #FDFE15
	Marron = #BF3604
	*/
}

.pergunta .link-resposta a:hover {
	background-color: #78AA39;
	background: linear-gradient(#78AA39, #78AA39); /* Standard syntax */
}

.resposta div{
	padding: 0;
}
.resposta .titulo-user-resposta{
	margin-top: 15px;
}
.resposta .data-pergunta{
	margin-bottom: 15px;
}
.resposta .titulo-resposta{
	padding: 10px;
	background-color: #efefef;
}


/*Fim do Bloco de Perguntas*/
.banner {
	margin-top: 10px;
	text-align: center;
}

.banner h3 {
	text-align: left;
}

.banner img {
	max-width: 100%;
}

.right {
	width: 25%;
	display: inline-block;
}

.ranking-user {
	text-align: justify;
	padding: 20px;
}

.rank-item {
	vertical-align: middle;
}

.rank-item p,.rank-item a {
	display: inline-block;
	vertical-align: top;
}

.rank-item a {
	color: #022601;
}

.rank-item img {
	max-width: 15px;
}

.ranking-serv {
	text-align: justify;
	padding: 20px;
}

/*RODAPÉ*/
.footer {
	background-color: #91C95A;
	border-bottom: 1px dotted #ccc;
	position: absolute;
	bottom: -75px;
	width: 100%;
}

.rodape {
	width: 98%;
	margin: 0 auto;
	
}

.institucional p {
	text-align: center;
	color: #000;
	padding: 5px;
}

/*P�gina de Servi�os*/
.bloco-servicos h3 {
	text-align: left;
}

.lista-servicos li {
	display: inline-block;
	width: 25%;
	margin-top: 15px;
}

.lista-servicos li a {
	color: #022601;
	font-weight: bold;
}

.lista-servicos li a img {
	max-width: 150px;
}

/*P�gina de Contato*/
.bloco-contato {
	text-align: left;
}

.bloco-contato h3 {
	text-align: left;
}

.bloco-contato form {
	width: 50%;
	margin-top: 10px;
	text-align: left;
	width: 80%;
}

.bloco-contato form label {
	vertical-align: middle;
}

.bloco-contato form input {
	margin: 10px 0;
	padding: 10px;
	width: 70%;
}

.bloco-contato form textarea {
	vertical-align: middle;
	width: 70%;
	margin: 10px 0;
	padding: 10px;
}

.bloco-contato form .bt-enviar {
	width: 50%;
	vertical-align: middle;
	font-weight: bold;
	cursor: pointer;
	color: #ffffff;
	border: none;
	border-radius: 5px;
	background-color: #52A202;
}

.bloco-contato form .bt-enviar:hover {
	background-color: #78AA39;
	background: linear-gradient(#78AA39, #78AA39); /* Standard syntax */
}

/*Pagina resposta*/
.bloco-resposta .pergunta a {
	display: inline-block;
	vertical-align: middle;
	padding: 5px;
	font-size: 14px;
	color: #022601;
}
.bloco-resposta .pergunta img{
	width: 150px;
	vertical-align: middle;
}
.bloco-resposta .pergunta a img {
	max-width: 150px;
	
}

.bloco-resposta .pergunta .pergunta-txt {
	display: inline-block;
	vertical-align: middle;
}

.bloco-resposta .pergunta p {
	display: inline-block;
	vertical-align: middle;
	font-size: 12px;
}

.bloco-resposta .resposta {
	border: 1px solid #eee;
	text-align: left;
	padding: 15px 20px;
	margin: 10px 0;
}

.bloco-resposta .resposta a img {
	max-width: 50px;
	display: inline-block;
	vertical-align: middle;
}

.bloco-resposta .resposta a {
	display: inline-block;
	vertical-align: middle;
	width: 20%;
	padding: 5px;
	font-size: 14px;
	color: #022601;
}

.bloco-resposta .resposta .resposta-txt {
	width: 70%;
	display: inline-block;
}

.resposta .date {
	text-align: right;
}

.resposta .pontuacao{
	display: block;
}
.resposta .pontuacao .estrela{
	display: inline-block;
	vertical-align: middle;
}

.resposta .pontuacao .estrela a{
	background-image: url("../images/rank/icon-estrela-vazia.png");
	background-repeat: no-repeat;
	background-size: 30px;
	height: 25px;
	width: 35px;
}
.resposta .pontuacao .estrela a:hover{
		background-image: url("../images/rank/icon-estrela-cheia.png");
	background-repeat: no-repeat;
	background-size: 30px;
}
.resposta .pontuacao .estrela a img{
	width: 35px;
}


